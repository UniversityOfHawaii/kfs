	<bean id="ContractsGrantsInvoiceDocumentErrorCategory" parent="ContractsGrantsInvoiceDocumentErrorCategory-parentBean" />

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.ContractsGrantsInvoiceDocumentErrorCategory" />
		<property name="titleAttribute" value="proposalNumber" />
		<property name="objectLabel" value="ContractsGrantsInvoiceDocumentErrorCategory" />
		<property name="attributes">
			<list>
				<ref bean="ContractsGrantsInvoiceDocumentErrorCategory-proposalNumber" />
				<ref bean="ContractsGrantsInvoiceDocumentErrorCategory-errorLogIdentifier" />
				<ref bean="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryIdentifier" />
				<ref bean="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryText" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-proposalNumber" parent="ContractsGrantsInvoiceDocumentErrorCategory-proposalNumber-parentBean" />

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-proposalNumber-parentBean" abstract="true"
		parent="ExternalizableAttributeDefinitionProxy"
		p:sourceExternalizableBusinessObjectInterface="org.kuali.kfs.integration.cg.ContractsAndGrantsBillingAward"
		p:sourceAttributeName="proposalNumber">
		<property name="name" value="proposalNumber" />
		<property name="maxLength" value="12" />
		<property name="validationPattern">
			<ref bean="NumericValidation" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="14" />
		</property>
	</bean>

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-errorLogIdentifier" parent="ContractsGrantsInvoiceDocumentErrorCategory-errorLogIdentifier-parentBean" />

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-errorLogIdentifier-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="errorLogIdentifier" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Error Log Identifier" />
		<property name="shortLabel" value="ErrLogId" />
		<property name="maxLength" value="12" />
		<property name="validationPattern">
			<ref bean="NumericValidation" />
		</property>
		<property name="control">
			<ref bean="HiddenControl" />
		</property>
	</bean>

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryIdentifier" parent="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryIdentifier-parentBean" />

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryIdentifier-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="validationCategoryIdentifier" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Error Category Identifier" />
		<property name="shortLabel" value="ErrCatId" />
		<property name="maxLength" value="12" />
		<property name="validationPattern">
			<ref bean="NumericValidation" />
		</property>
		<property name="control">
			<ref bean="HiddenControl" />
		</property>
	</bean>

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryText" parent="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryText-parentBean" />

	<bean id="ContractsGrantsInvoiceDocumentErrorCategory-validationCategoryText-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="validationCategoryText" />
		<property name="label" value="Validation Category Text" />
		<property name="shortLabel" value="ValText" />
		<property name="maxLength" value="200" />
		<property name="validationPattern">
			<ref bean="AnyCharacterWithWhitespaceValidation" />
		</property>
		<property name="control">
			<bean parent="TextareaControlDefinition" p:rows="2" p:cols="100"/>
		</property>
	</bean>

</beans>
