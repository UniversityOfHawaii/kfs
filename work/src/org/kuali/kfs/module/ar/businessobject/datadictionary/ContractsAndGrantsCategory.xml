	<bean id="ContractsAndGrantsCategory" parent="ContractsAndGrantsCategory-parentBean" />

	<bean id="ContractsAndGrantsCategory-parentBean" abstract="true" parent="BusinessObjectEntry">

		<property name="businessObjectClass" value="org.kuali.kfs.module.ar.businessobject.ContractsAndGrantsCategory" />
		<property name="objectLabel" value="ContractsAndGrantsCategory" />
		<property name="titleAttribute" value="categoryCode"/>
		<property name="attributes">
			<list>
				<ref bean="ContractsAndGrantsCategory-categoryCode" />
				<ref bean="ContractsAndGrantsCategory-categoryName" />
				<ref bean="ContractsAndGrantsCategory-categoryDescription" />
				<ref bean="ContractsAndGrantsCategory-categoryObjectCodes" />
				<ref bean="ContractsAndGrantsCategory-categoryConsolidations" />
				<ref bean="ContractsAndGrantsCategory-categoryLevels" />
				<ref bean="ContractsAndGrantsCategory-indirectCostIndicator" />
				<ref bean="ContractsAndGrantsCategory-active" />
			</list>
		</property>
		<property name="inquiryDefinition">
      		<ref bean="ContractsAndGrantsCategory-inquiryDefinition"/>
    	</property>
		<property name="lookupDefinition">
			<ref bean="ContractsAndGrantsCategory-lookupDefinition" />
		</property>	
	</bean>

	<!-- Attribute Definitions -->


	<bean id="ContractsAndGrantsCategory-categoryCode" parent="ContractsAndGrantsCategory-categoryCode-parentBean" />

	<bean id="ContractsAndGrantsCategory-categoryCode-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="categoryCode" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Cost Category Code" />
		<property name="shortLabel" value="Cost Category Code" />
		<property name="maxLength" value="4" />
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="5" />
		</property>
	</bean>

	<bean id="ContractsAndGrantsCategory-categoryName" parent="ContractsAndGrantsCategory-categoryName-parentBean" />

	<bean id="ContractsAndGrantsCategory-categoryName-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="categoryName" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Cost Category Name" />
		<property name="shortLabel" value="Cost Category Name" />
		<property name="maxLength" value="128" />
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="50" />
		</property>
	</bean>

	<bean id="ContractsAndGrantsCategory-categoryDescription" parent="ContractsAndGrantsCategory-categoryDescription-parentBean" />


	<bean id="ContractsAndGrantsCategory-categoryDescription-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="categoryDescription" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Cost Category Description" />
		<property name="shortLabel" value="Cost Category Description" />
		<property name="maxLength" value="45" />
		<property name="required" value="false" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="50" />
		</property>
	</bean>

	<bean id="ContractsAndGrantsCategory-categoryObjectCodes" parent="ContractsAndGrantsCategory-categoryObjectCodes-parentBean" />

	<bean id="ContractsAndGrantsCategory-categoryObjectCodes-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="categoryObjectCodes" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Cost Category Object Codes" />
		<property name="shortLabel" value="Cost Category Object Codes" />
		<property name="maxLength" value="256" />
		<property name="required" value="true" />
	    <property name="constraintText" value="Must be 10 digits"/>
    	<property name="summary" value="Travel Account Number"/>
    	<property name="description" value="Travel Account Number"/>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="50" />
		</property>
	</bean>

	<bean id="ContractsAndGrantsCategory-categoryConsolidations" parent="ContractsAndGrantsCategory-categoryConsolidations-parentBean" />

	<bean id="ContractsAndGrantsCategory-categoryConsolidations-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="categoryConsolidations" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Cost Category Consolidations" />
		<property name="shortLabel" value="Cost Category Consolidations" />
		<property name="maxLength" value="256" />
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="50" />
		</property>
	</bean>

	<bean id="ContractsAndGrantsCategory-categoryLevels" parent="ContractsAndGrantsCategory-categoryLevels-parentBean" />

	<bean id="ContractsAndGrantsCategory-categoryLevels-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="categoryLevels" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Cost Category Levels" />
		<property name="maxLength" value="256" />
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="50" />
		</property>
	</bean>
	
	<bean id="ContractsAndGrantsCategory-indirectCostIndicator" parent="ContractsAndGrantsCategory-indirectCostIndicator-parentBean" />

	<bean id="ContractsAndGrantsCategory-indirectCostIndicator-parentBean" abstract="true" parent="GenericAttributes-genericBoolean">
		<property name="name" value="indirectCostIndicator" />
		<property name="label" value="Indirect Cost Indicator" />
		<property name="shortLabel" value="Indirect Cost Indicator" />
	</bean>


	<bean id="ContractsAndGrantsCategory-active" parent="ContractsAndGrantsCategory-active-parentBean" />

	<bean id="ContractsAndGrantsCategory-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
		<property name="name" value="active" />
	</bean>

	<bean id="ContractsAndGrantsCategory-lookupDefinition" parent="ContractsAndGrantsCategory-lookupDefinition-parentBean" />

	<bean id="ContractsAndGrantsCategory-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Cost Category Lookup" />

		<property name="defaultSort">
			<bean parent="SortDefinition">
			</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="categoryCode" />
				<bean parent="FieldDefinition" p:attributeName="categoryName" />
				<bean parent="FieldDefinition" p:attributeName="indirectCostIndicator" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="categoryCode" />
				<bean parent="FieldDefinition" p:attributeName="categoryName" />
				<bean parent="FieldDefinition" p:attributeName="categoryDescription" />
				<bean parent="FieldDefinition" p:attributeName="categoryObjectCodes" />
				<bean parent="FieldDefinition" p:attributeName="indirectCostIndicator" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>
	
	<!-- Business Object Inquiry Definition -->
	
	<bean id="ContractsAndGrantsCategory-inquiryDefinition" parent="ContractsAndGrantsCategory-inquiryDefinition-parentBean"/>
	
	<bean id="ContractsAndGrantsCategory-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
	  <property name="title" value="Cost Category Inquiry"/>
	  <property name="inquirySections">
	    <list>
	      <bean parent="InquirySectionDefinition">
	        <property name="title" value="Cost Category"/>
	        <property name="numberOfColumns" value="1"/>
	        <property name="inquiryFields">
	          <list>
	            <bean parent="FieldDefinition" p:attributeName="categoryCode"/>
	            <bean parent="FieldDefinition" p:attributeName="categoryName"/>
	            <bean parent="FieldDefinition" p:attributeName="categoryDescription"/>
	            <bean parent="FieldDefinition" p:attributeName="categoryObjectCodes"/>
	            <bean parent="FieldDefinition" p:attributeName="categoryConsolidations"/>
	            <bean parent="FieldDefinition" p:attributeName="categoryLevels"/>
	            <bean parent="FieldDefinition" p:attributeName="indirectCostIndicator"/>
	            <bean parent="FieldDefinition" p:attributeName="active"/>
	          </list>
	        </property>
	      </bean>
	    </list>
	  </property>
	</bean>
  
</beans>
